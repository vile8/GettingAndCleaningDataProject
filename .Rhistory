columnmean <- function(y, removeNA = TRUE) {
#number of columns
nc <- ncol(y)
#create an empty numeric vector with 0 as default values for
#each of the column "nc" means
means <- numeric(nc)
#loop through each column using numebr of columns defined "nc"
for(i in 1:nc){
#assign to each of the means vector the value of
means[i] <- mean(y[,i], na.rm = removeNA)
}
